<div class="LayoutPopup ARIARoleMain">

    <div class="Header">
        <h1>
            $Text{"History of"} $Config{"ITSMWorkOrder::Hook"} $QData{"ChangeNumber"}-$QData{"WorkOrderNumber"}: $QData{"WorkOrderTitle","60"}
        </h1>
        <p>
            <a class="CancelClosePopup" href="#">$Text{"Cancel & close window"}</a>
        </p>
    </div>
    <div class="Content">

        <h2>$Text{"Detailed history information of"} $Data{"HistoryType"}</h2>
        <fieldset class="TableLike FixedLabel">

            <label>$Text{"Modified"}:</label>
            <div class="Field">
                $QData{"Fieldname"}
            </div>
            <div class="Clear"></div>

            <label>$Text{"Old Value"}:</label>
            <div class="Field">
                $Data{"ContentOld"}
            </div>
            <div class="Clear"></div>

            <label>$Text{"New Value"}:</label>
            <div class="Field">
                $Data{"ContentNew"}
            </div>
            <div class="Clear"></div>

            <label>$Text{"Changed"}:</label>
            <div class="Field">
                $TimeLong{"$QData{"ChangeTime"}"}
            </div>
            <div class="Clear"></div>

            <label>$Text{"Changed by"}:</label>
            <div class="Field" title="$QData{"UserLogin"} ($QData{"UserFirstname"} $QData{"UserLastname"})">
                $QData{"UserLogin"} ($QData{"UserFirstname"} $QData{"UserLastname"})
            </div>
            <div class="Clear"></div>

        </fieldset>
    </div>
    <div class="Footer">
    </div>
</div>
